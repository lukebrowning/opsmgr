frontend kibana_ssl-front
bind *:8443
    option tcplog
    mode tcp
    default_backend kibana_ssl-back

backend kibana_ssl-back
    mode tcp
    balance source
    option ssl-hello-chk
    server {{ groups['kibana'][0] }} {{ hostvars[groups['kibana'][0]]['container_address'] }}:8443 check port 8443 inter 12000 rise 2 fall 2
